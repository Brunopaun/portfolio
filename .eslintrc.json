{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended",
  ],
  "globals": {
    "React":"readonly"
  },
  "rules": {
    "no-unused-vars":"warn",
    "comma-spacing":"warn",
    "object-property-newline":"warn",
    "sort-keys":"warn",
    "sort-vars":"warn",
    "no-multiple-empty-lines":"warn",
    "quotes":["warn", "double"],
    "semi":["warn", "never"],
  }
}
